{
    "useTabs": false,
    "typescript": {
        "indentWidth": 4,
        "quoteStyle": "preferDouble",
        "useBraces": "always"
    },
    "json": {
        "indentWidth": 4
    },
    "markdown": {
    },
    "toml": {
        "indentWidth": 4
    },
    "includes": [
        "**/*.{ts,json,md,toml}"
    ],
    "excludes": [
        "**/node_modules",
        "**/*-lock.json"
    ],
    "plugins": [
        "https://plugins.dprint.dev/typescript-0.68.0.wasm",
        "https://plugins.dprint.dev/json-0.15.1.wasm",
        "https://plugins.dprint.dev/markdown-0.13.1.wasm",
        "https://plugins.dprint.dev/toml-0.5.4.wasm"
    ]
}
